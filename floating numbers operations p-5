#include<iostream>
using namespace std;
int main(){
    float a,b,c;
    cout<<"enter a";
    cin>>a;
      cout<<"enter b";
    cin>>b;
    
    c=(a+b);
    cout<<"addition:"<<c<<endl;
    
    c=(a-b);
    cout<<"subtraction:"<<c<<endl;
    
    c=(a*b);
    cout<<"Multiplication:"<<c<<endl;
    
    c=(a/b);
    cout<<"Division:"<<c<<endl;

    
}
